{
  "expo": {
    "name": "swiggy",
    "slug": "swiggy",
    "scheme": "swiggy_clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "initialRoute": "index",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/con.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kaushikhardik35.swiggy"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8ce9550e-151c-45b8-be40-706bf7210820"
      }
    }
  }
}
